{
  "name": "unplugin-alpinejs-component",
  "version": "0.0.5",
  "description": "Combine Alpinejs and Component.",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "default": "./dist/index.cjs"
    },
    "./defineComponent": {
      "development": "./src/defineComponent.ts",
      "default": "./dist/defineComponent.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npx unbuild",
    "lint": "npx oxlint@latest",
    "test": "npx tsx tests/index.ts",
    "prepublishOnly": "npm test && npm run build"
  },
  "keywords": [
    "alpinejs",
    "alpine",
    "webcomponent",
    "custom-element",
    "framework",
    "unplugin",
    "plugin",
    "vite",
    "webpack",
    "rollup",
    "rspack",
    "esbuild"
  ],
  "author": "tingyuan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lovetingyuan/alpinejs-component/issues"
  },
  "homepage": "https://github.com/lovetingyuan/alpinejs-component#readme",
  "dependencies": {
    "cssnano": "^6.0.1",
    "html-minifier-terser": "^7.2.0",
    "parse5": "^7.1.2",
    "unplugin": "^1.5.1",
    "validate-element-name": "2.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "npm-run-all": "^4.1.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0"
  },
  "peerDependencies": {
    "@types/alpinejs": "^3.13.5",
    "alpinejs": ">=3.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lovetingyuan/alpinejs-component.git"
  }
}
